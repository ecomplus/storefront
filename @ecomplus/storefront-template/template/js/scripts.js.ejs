<% 
  const requireThemeFiles = () => {
    let js = ''
    for (const themeColl in theme) {
      const themeName = theme[themeColl]
      if (themeName && themeName.length > 1) {
        js += `require('./${themesPath}/${themeName}/product')`
      }
    }
    return js
  }
%>

<%- requireThemeFiles() %>
